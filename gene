         cursor.className = 'custom-cursor';
         document.body.appendChild(cursor);
         const elementsToHover = document.querySelectorAll('.news-elem img, .gen, .news-cover');
         elementsToHover.forEach(element => {
         element.addEventListener('mouseenter', () => {
         cursor.classList.add('active');
         element.style.cursor = 'none';
         });
         element.addEventListener('mouseleave', () => {
         cursor.classList.remove('active');
         element.style.cursor = 'auto';
         });
         });
         document.addEventListener('mousemove', (e) => {
         cursor.style.left = e.clientX + 'px';
         cursor.style.top = e.clientY + 'px';
         });</script><script>document.addEventListener("DOMContentLoaded", function () {
         const selectToggleBtn = document.getElementById("selectToggleBtn1");
         const newsElems = document.querySelectorAll(".news-elem");
         selectToggleBtn.addEventListener("click", function () {
         const randomIndices = generateRandomIndices(newsElems.length, 5);
         newsElems.forEach((newsElem, index) => {
         if (randomIndices.includes(index)) {
         newsElem.classList.add("selected");
         } else {
         newsElem.classList.remove("selected");
         }
         });
         Swal.fire({
         title: '5 Items seleccionados',
         text: '5 Items han sido seleccionados aleatoriamente.',
         icon: 'success',
         showConfirmButton: false,
         timer: 1000,
         timerProgressBar: true,
         customClass: {
         popup: 'custom-swal-popup',
         title: 'custom-swal-title',
         },
         });
         });
         });
         function generateRandomIndices(max, count) {
         const indices = [];
         while (indices.length < count) {
         const randomIndex = Math.floor(Math.random() * max);
         if (!indices.includes(randomIndex)) {
         indices.push(randomIndex);
         }
         }
         return indices;
         }</script><script>document.addEventListener("DOMContentLoaded", function () {
