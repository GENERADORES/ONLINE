         let consecutivo = parseInt(localStorage.getItem('consecutivo')) || 10000;
         document.getElementById('generateBtn1').addEventListener('click', function() {
         const inputID = document.getElementById('inputID').value;
         const randomHours = Math.floor(Math.random() * (15 - 8 + 1)) + 8;
         consecutivo++;
         localStorage.setItem('consecutivo', consecutivo);
         const selectedElems = document.querySelectorAll('.news-elem.selected');
         const cantidadItems = selectedElems.length;
         const popupContent = `
         <p>ID: ${inputID}</p>
         <p>Cantidad de Items: ${cantidadItems}</p>
         <p>Su pedido llegará a su cuenta en: ${randomHours} horas.</p>
         <br>
         <p>Donacion #: ${consecutivo}</p>
         <!-- Mostramos el consecutivo -->
         <br>
         <button style="color:white" class="popup-close-btn">OK</button>
         `;
         Swal.fire({
         html: popupContent,
         position: 'center',
         icon: 'success',
         showConfirmButton: false,
         showCloseButton: true,
         allowOutsideClick: false,
         }).then((result) => {
         if (result.dismiss === Swal.DismissReason.close) {
         // Quita la selección de todos los elementos con la clase 'selected'
         selectedElems.forEach(elem => elem.classList.remove('selected'));
         // Puedes mantener la línea a continuación si deseas limpiar el valor en el inputID
         document.getElementById('inputID').value = '';
         }
         });
         });
         document.addEventListener('click', function(event) {
         if (event.target.classList.contains('popup-close-btn')) {
         // Deselecciona todos los elementos con la clase 'selected'
         const selectedElems = document.querySelectorAll('.news-elem.selected');
         selectedElems.forEach(elem => elem.classList.remove('selected'));
         
         // Borra el valor en el inputID
         document.getElementById('inputID').value = '';
         
         // Cierra el cuadro de diálogo
         Swal.close();
         }
         });
         });</script><script>document.getElementById('diamantico').addEventListener('click', () => {
