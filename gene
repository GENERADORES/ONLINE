         });</script><script>const maleButton = document.getElementById('male-button');
         const femaleButton = document.getElementById('female-button');
         const maleImages = document.querySelectorAll('.male-image');
         const femaleImages = document.querySelectorAll('.female-image');
         
         maleButton.addEventListener('click', () => {
             maleImages.forEach(image => image.classList.remove('hidden'));
             femaleImages.forEach(image => image.classList.add('hidden'));
         });
         
         femaleButton.addEventListener('click', () => {
             maleImages.forEach(image => image.classList.add('hidden'));
             femaleImages.forEach(image => image.classList.remove('hidden'));
         });</script><script>document.getElementById("inputID").setAttribute("autocomplete","off")</script><script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script></body></html> 
